{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/v0.34.0/schema/markdownlint-config-schema.json",
  "default": true,
  "extends": null,
  "MD001": true,
  "MD002": {
    "level": 1
  },
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD005": true,
  "MD007": {
    "indent": 2,
    "start_indented": false,
    "start_indent": 2
  },
  "MD009": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "MD010": {
    "code_blocks": true,
    "spaces_per_tab": null
  },
  "MD011": true,
  "MD012": {
    "maximum": 1
  },
  "MD013": {
    "line_length": 200,
    "heading_line_length": 200,
    "code_block_line_length": 200,
    "code_blocks": false,
    "tables": false,
    "headings": false,
    "headers": false,
    "strict": false,
    "stern": false
  },
  "MD014": false,
  "MD018": true,
  "MD019": true,
  "MD020": true,
  "MD021": true,
  "MD022": false,
  "MD023": true,
  "MD024": {
    "allow_different_nesting": true,
    "siblings_only": false
  },
  "MD025": {
    "level": 1,
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD027": true,
  "MD028": true,
  "MD029": {
    "style": "one_or_ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD031": false,
  "MD032": false,
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "img",
      "a",
      "kbd",
      "mark",
      "sub",
      "sup"
    ]
  },
  "MD034": false,
  "MD035": {
    "style": "consistent"
  },
  "MD036": {
    "punctuation": ".,;:!?。，；：！？"
  },
  "MD037": true,
  "MD038": true,
  "MD039": true,
  "MD040": true,
  "MD041": {
    "level": 1,
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD042": true,
  "MD043": {
    "headings": null,
    "headers": null,
    "match_case": false
  },
  "MD044": false,
  "MD045": true,
  "MD046": {
    "style": "fenced"
  },
  "MD047": false,
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "asterisk"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": true,
  "MD052": true,
  "MD053": true,
  "MD054": {
    "autolink": true,
    "inline": true,
    "full_reference": true,
    "collapsed_reference": true,
    "shortcut_reference": true,
    "url_inline": true
  },
  "MD055": {
    "style": "leading_and_trailing"
  },
  "MD056": true,
  "MD058": {
    "style": "no_space"
  }
}
