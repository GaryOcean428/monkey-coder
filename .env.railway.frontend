# Railway Deployment Environment Variables for Frontend Build

# Required for Next.js build to succeed
NEXTAUTH_URL=https://coder.fastmonkey.au
NEXTAUTH_SECRET=your-production-secret-here-change-this
NEXT_PUBLIC_API_URL=https://coder.fastmonkey.au
NEXT_PUBLIC_APP_URL=https://coder.fastmonkey.au

# Optional: Force static export (already handled by railpack.json)
NEXT_OUTPUT_EXPORT=true

# Optional: Database URL (can be empty for static export)
DATABASE_URL=

# Optional: Stripe keys (only if using billing)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key
STRIPE_SECRET_KEY=sk_test_your_key

# Railway-specific variables (set automatically)
RAILWAY_ENVIRONMENT=production
PORT=3000

# Optional: Performance tuning
NODE_ENV=production
ENABLE_SERVE_FRONTEND=true

# Optional: Disable telemetry
NEXT_TELEMETRY_DISABLED=1