{
  "name": "monkey-coder-cli",
  "version": "1.5.0",
  "description": "CLI for Monkey Coder - AI-powered code generation platform. Requires account at https://coder.fastmonkey.au or local backend.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "monkey": "./dist/cli.js",
    "monkey-coder": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src --ext .ts,.tsx,.js || echo 'No JS files to lint'",
    "lint:fix": "eslint src --ext .ts,.tsx,.js --fix || echo 'No JS files to lint'",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "JEST_JUNIT_OUTPUT_DIR=coverage/junit JEST_JUNIT_OUTPUT_NAME=jest-junit.xml jest --coverage --passWithNoTests --reporters=default --reporters=jest-junit",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "dependencies": {
    "@sentry/node": "^9.42.0",
    "axios": "^1.11.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "eventsource-parser": "^3.0.3",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.8.2",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "sharp": "^0.34.3",
    "supports-color": "^10.1.0",
    "term-img": "^7.0.0",
    "terminal-link": "^4.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
