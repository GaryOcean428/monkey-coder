{
  "name": "monkey-coder-cli",
  "version": "1.3.1",
  "description": "TypeScript CLI for Monkey Coder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "monkey": "./dist/cli.js",
    "monkey-coder": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src --ext .js || echo 'No JS files to lint'",
    "lint:fix": "eslint src --ext .js --fix || echo 'No JS files to lint'",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "dependencies": {
    "@sentry/node": "^9.42.0",
    "axios": "^1.11.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "eventsource-parser": "^3.0.3",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.8.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
