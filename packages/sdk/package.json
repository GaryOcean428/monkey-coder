{
  "name": "monkey-coder-sdk",
  "version": "1.3.6",
  "description": "Official TypeScript/JavaScript and Python SDK for Monkey Coder - AI-powered code generation and analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build && npm run build:python",
    "build:ts": "tsc --build",
    "build:python": "cd src/python && python setup.py build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo src/python/build src/python/dist",
    "lint": "eslint src --ext .ts,.tsx,.js || echo 'No JS files to lint'",
    "lint:fix": "eslint src --ext .ts,.tsx,.js --fix || echo 'No JS files to lint'",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "JEST_JUNIT_OUTPUT_DIR=coverage/junit JEST_JUNIT_OUTPUT_NAME=jest-junit.xml jest --coverage --passWithNoTests --reporters=default --reporters=jest-junit",
    "prepublishOnly": "npm run clean && npm run build",
    "examples:node": "cd examples/node && npm install && npm start",
    "examples:bun": "cd examples/bun && bun install && bun run start",
    "examples:deno": "cd examples/deno && deno run --allow-net index.ts",
    "examples:python": "cd examples/python && pip install -r requirements.txt && python main.py"
  },
  "dependencies": {
    "@types/node": "^24.1.0",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "code-generation",
    "code-analysis",
    "sdk",
    "api",
    "monkey-coder",
    "typescript",
    "python",
    "automation",
    "development-tools"
  ],
  "author": {
    "name": "Monkey Coder",
    "email": "support@monkeycoder.dev",
    "url": "https://monkeycoder.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monkey-coder/sdk.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/monkey-coder/sdk/issues"
  },
  "homepage": "https://docs.monkeycoder.dev/sdk",
  "engines": {
    "node": ">=16.0.0"
  }
}
