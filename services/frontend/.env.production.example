# Production Environment Variables
# These are used when building for production deployment

# Backend API URL (Railway deployment)
# This should point to your FastAPI backend service
# For Railway: https://monkey-coder-backend-production.up.railway.app
# For custom domain: https://coder.fastmonkey.au (if backend is on same domain)
NEXT_PUBLIC_API_URL=https://monkey-coder-backend-production.up.railway.app

# Application URL (for redirects and callbacks)
NEXT_PUBLIC_APP_URL=https://coder.fastmonkey.au

# Analytics (optional)
NEXT_PUBLIC_GA_ID=

# Feature flags
NEXT_PUBLIC_ENABLE_SOCIAL_LOGIN=false
NEXT_PUBLIC_ENABLE_API_KEYS=true
