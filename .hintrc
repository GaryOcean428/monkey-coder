{
  "extends": ["development"],
  "hints": {
    "no-inline-styles": "off"
  },
  "ignoredUrls": [
    "**/.next/**",
    "**/out/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**"
  ],
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!**/.next/**",
        "!**/out/**",
        "!**/node_modules/**",
        "!**/dist/**",
        "!**/build/**"
      ]
    }
  }
}
