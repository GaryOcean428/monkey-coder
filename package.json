{
  "name": "monkey-coder",
  "version": "1.1.0",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "packages/*",
    "services/*",
    "docs"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -At run build",
    "test": "yarn workspaces foreach -At run test",
    "test:watch": "yarn workspaces foreach -pi run test:watch",
    "test:coverage": "yarn workspaces foreach -At run test:coverage",
    "lint": "yarn workspaces foreach -At run lint",
    "lint:fix": "yarn workspaces foreach -At run lint:fix",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --ignore .yarn",
    "lint:md:fix": "markdownlint \"**/*.md\" --ignore node_modules --ignore .yarn --fix",
    "typecheck": "yarn workspaces foreach -At run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "clean": "yarn workspaces foreach -At run clean",
    "clean:deps": "yarn workspaces foreach -At exec rm -rf node_modules && rm -rf node_modules",
    "dev": "yarn workspaces foreach -pi run dev",
    "release": "yarn changeset publish",
    "release:version": "yarn changeset version",
    "publish-npm": "yarn workspace monkey-coder-cli publish --access public && yarn workspace monkey-coder-sdk publish --access public",
    "publish-pypi": "cd packages/core && python -m build && python -m twine upload dist/* && cd ../sdk/src/python && python -m build && python -m twine upload dist/*",
    "docs:dev": "yarn workspace docs run dev",
    "docs:build": "yarn workspace docs run build",
    "docs:serve": "yarn workspace docs run serve",
    "docs:validate-links": "yarn workspace docs run validate-links"
  },
  "devDependencies": {
    "@algolia/client-search": "^5.35.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@jest/globals": "^30.0.5",
    "@sentry/vite-plugin": "^4.0.1",
    "@sentry/webpack-plugin": "^4.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "acorn": "^8.15.0",
    "algoliasearch": "^5.35.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "linkinator": "^6.1.4",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "search-insights": "^2.17.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@mdx-js/react": "^3.1.0",
    "@prisma/client": "^6.14.0",
    "@sentry/browser": "^9.42.0",
    "@sentry/node": "^9.42.0",
    "@sentry/profiling-node": "^9.42.0",
    "@sentry/react": "^9.42.0",
    "@tanstack/react-query": "4.40.1",
    "corepack": "^0.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.101.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
