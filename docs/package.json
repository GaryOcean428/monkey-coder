{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "validate-links": "linkinator build --recurse --format csv",
    "validate-links:json": "linkinator build --recurse --format json",
    "dev": "docusaurus start",
    "serve:4000": "http-server build -p 4000 -s",
    "validate-links:http": "linkinator http://localhost:4000/monkey-coder/ --recurse --skip 'mailto:|tel:'",
    "validate-links:served": "concurrently -k -s first -n serve,check -c blue,green \"http-server build -p 4000 -s\" \"wait-on -t 20000 http://localhost:4000/monkey-coder/ && linkinator http://localhost:4000/monkey-coder/ --recurse --skip 'mailto:|tel:'\""
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-live-codeblock": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@webcontainer/api": "^1.6.1",
    "clsx": "^2.1.1",
    "iframe-resizer-react": "^5.1.5",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-iframe": "^1.8.5"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^9.1.2",
    "http-server": "^14.1.1",
    "linkinator": "^6.1.4",
    "typescript": "~5.8.3",
    "wait-on": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}